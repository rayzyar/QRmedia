		<servlet-class>com.fyp.qrmedia.CreateProfileServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CreateProfile</servlet-name>
		<url-pattern>/createprofile</url-pattern>
	</servlet-mapping>
	<servlet>
