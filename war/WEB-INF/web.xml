		<servlet-class>com.fyp.qrmedia.ClearIPListServlet</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>Subscribe</servlet-name>
		<servlet-class>com.fyp.qrmedia.SubscribeServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Subscribe</servlet-name>
		<url-pattern>/subscribe</url-pattern>
	</servlet-mapping>

	
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
